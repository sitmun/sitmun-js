{
  "name": "sitmun-js",
  "version": "0.0.1",
  "description": "An universal client for the SITMUN public api written in Typescript",
  "main": "./dist/sitmun.cjs",
  "module": "./dist/sitmun.module.js",
  "unpkg": "./dist/sitmun.umd.js",
  "source": "src/sitmun.ts",
  "repository": "github:sitmun/sitmun-js",
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json",
