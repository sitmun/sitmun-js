    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.4.0",
    "microbundle": "^0.13.0",
    "nock": "^12.0.3"
  },
  "keywords": [
    "sitmun",
    "javascript",
    "typescript",
    "api",
    "client",
    "universal"
  ],
  "author": "Francisco J Lopez-Pellicer <fjlopez@unizar.es>",
  "license": "MIT",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "isomorphic-unfetch": "3.1.0",
    "querystringify": "2.2.0"
  }
}
